---
import Layout from '../layouts/Layout.astro';
import Navbar from '../components/Navbar.astro';
import Footer from '../components/Footer.astro';
import Button from "../components/Button.astro";
---
<Layout title="Piercing Parlour | Black Cat Tattoo & <Pierci></Pierci>ng Parlour">
    <Navbar/>

    <section class="pt-36 pb-20 px-6 md:px-12 lg:px-20 text-white">
        <h1 class="text-7xl lg:text-9xl font-semibold text-center mb-16">Piercing Parlour</h1>

        <h2 class="text-7xl font-semibold text-center mb-16">Artists</h2>
        <h2 class="text-7xl font-semibold text-center mb-16">Gallery</h2>
        <h2 class="text-7xl font-semibold text-center mb-16">Pricing</h2>

        <div class="p-10 items-center justify-center text-center">
            <Button text="Contact Us!" href="/contact-us"/>
        </div>
    </section>

    <Footer/>
</Layout>